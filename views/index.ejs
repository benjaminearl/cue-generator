!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>Twitch Script</title>
    <style>
      body {
        font-family: Arial, Helvetica, sans-serif;
        font-size: 64px;
      }

      #container {
        margin-left: 50px;
      }
      
      .actor {
        padding-bottom: 5 px;
      }

      #actor1 {
        color: red;
      }

      #actor2 {
        color: blue;
      }

      #actor3 {
        color: green;
      }

      #actor4 {
        color: indigo;
      }

      #actor4 {
        color: orange;
      }

    </style>
  </head>
  <body>
    <div id="container">
      <div class="actor" id="actor1"><p>#1: <span id="1"></span></p></div>
      <div class="actor" id="actor2"><p>#2: <span id="2"></span></p></div>
      <div class="actor" id="actor3"><p>#3: <span id="3"></span></p></div>
      <div class="actor" id="actor4"><p>#4: <span id="4"></span></p></div>
      <div class="actor" id="actor5"><p>#5: <span id="5"></span></p></div>
    </div>
  </body>
</html> 